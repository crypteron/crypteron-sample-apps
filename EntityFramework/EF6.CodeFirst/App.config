<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="crypteronConfig" type="Crypteron.CrypteronConfig, CipherCore" />
  </configSections>
  <crypteronConfig>
    <myCrypteronAccount appSecret="Demonstrates_AppSecret_Added_Via_Program.cs_Main()" />
  </crypteronConfig>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <connectionStrings>
    <!--Example: Local SDF file (SQL CE). You need to create the SDF file -->
    <!--<add name="DatabaseEntities" connectionString="Data Source=|DataDirectory|SQL-CE-database.sdf" providerName="System.Data.SqlServerCe.4.0" />-->

    <!--Example: LocalDB. Already made for you in these sample apps-->
    <add name="DatabaseEntities" connectionString="data source=(localdb)\mssqllocaldb;Integrated Security=true;AttachDBFilename=|DataDirectory|CipherDbCodeFirst.mdf;" providerName="System.Data.SqlClient" />

    <!--Example: SQL Server. You need to replace all the [ ] tokens below -->
    <!--<add name="DatabaseEntities" connectionString="Server=[serverName];Database=[myDataBase];User Id=[LoginForDb];Password=[myPassword];" providerName="System.Data.SqlClient" />-->

    <!--Example: Azure SQL. You need to replace all the [ ] tokens below -->
    <!--<add name="DatabaseEntities" connectionString="Server=tcp:[serverName].database.windows.net;Database=[myDataBase];User ID=[LoginForDb]@[serverName];Password=[myPassword];Trusted_Connection=False;Encrypt=True;" providerName="System.Data.SqlClient" />-->
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Data.OData" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.0.0" newVersion="5.2.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.WindowsAzure.Storage" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
