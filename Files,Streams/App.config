<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="crypteronConfig" type="Crypteron.CrypteronConfig, CipherCore" requirePermission="false" />
  </configSections>
  <crypteronConfig>
    <myCrypteronAccount appSecret="Replace_this_with_app_secret_from_https://my.crypteron.com" />
    <cipherStorConfig reportEventEveryXBytes="10" />
  </crypteronConfig>
  <connectionStrings>
    <!--This needs to be set if you want the CipherStor sample app to work with Azure Blob Storage-->
    <add name="StorageConnectionString" connectionString="DefaultEndpointsProtocol=https;AccountName=yourBlobStorageAccountName;AccountKey=" />
  </connectionStrings>
</configuration>
